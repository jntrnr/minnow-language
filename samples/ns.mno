namespace Bob
feature Cool
  x : string
  feature Inner
    y : string
  end
end

namespace Math.Complex
feature Complex_Number
  real : double
  imaginary : double
end

namespace global

action main
  x : Bob.Cool
  y : Math.Complex.Complex_Number

  o : object <+ new Bob.Cool() <+ new Math.Complex.Complex_Number()
  o.(Bob.Cool).x = "Nice"

  x.x = "Bob Cool"
  y.real = 10.0
  y.imaginary = 100.0
end
